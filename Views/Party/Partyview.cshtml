@model IEnumerable<SFO.Models.Party>

@{
    ViewData["Title"] = "Party List";
}

<h1>Party List</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var party in Model)
        {
            <tr>
                <td>@party.PartyID</td>
                <td>@party.PartyName</td>
          
            </tr>
        }
    </tbody>
</table>